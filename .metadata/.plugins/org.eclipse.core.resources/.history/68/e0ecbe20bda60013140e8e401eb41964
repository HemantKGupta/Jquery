    <script>
    $( document ).ready(function() {
    	$( "a" ).addClass( "test" );
        $( "a" ).click(function( event ) {
        	alert( "The link will no longer take you to jquery.com" );
            event.preventDefault();
        });
     	//$( "a" ).removeClass( "test" );
    });
   
 
    </script>

